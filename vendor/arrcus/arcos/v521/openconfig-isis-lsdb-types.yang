    description
      "Base identity for an ISIS TLV 141 SUB-TLV type.";
  }

  identity ISIS_TLV222_SUBTLVS_TYPE {
    base "ISIS_SUBTLV_TYPE";
    description
      "Base identity for an ISIS TLV 222 SUB-TLV type.";
  }

  identity ISIS_TLV223_SUBTLVS_TYPE {
    base "ISIS_SUBTLV_TYPE";
    description
      "Base identity for an ISIS TLV 223 SUB-TLV type.";
  }

