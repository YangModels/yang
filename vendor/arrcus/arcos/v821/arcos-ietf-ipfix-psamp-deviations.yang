    description "Not applicable in this implementation";
  }

  deviation "/ietf-ipfix:ipfix/ietf-ipfix:selectionProcess/ietf-ipfix:selector/ietf-ipfix:packetsDropped" {
    deviate not-supported;
    description "Not applicable in this implementation";
  }

  deviation "/ietf-ipfix:ipfix/ietf-ipfix:selectionProcess/ietf-ipfix:selector/ietf-ipfix:Method/ietf-ipfix:selectAll" {
    deviate not-supported;
    description "Not supported in this release";
  }

  deviation "/ietf-ipfix:ipfix/ietf-ipfix:selectionProcess/ietf-ipfix:selector/ietf-ipfix:Method/ietf-ipfix:sampTimeBased" {
