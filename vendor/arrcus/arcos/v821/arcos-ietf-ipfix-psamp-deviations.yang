module arcos-ietf-ipfix-psamp-deviations {
  yang-version 1.1;
  namespace "http://yang.arrcus.com/arcos/ietf/ipfix/psamp/deviations";
  prefix arc-ietf-ipfix-psamp-dev;

  import arcos-ietf-ipfix-psamp-augments {
    prefix arc-ietf-ipfix;
  }

  import ietf-ipfix-psamp {
    prefix ietf-ipfix;
  }

  import ietf-inet-types {
    prefix inet;
  }

  organization
    "Arrcus, Inc.";

  contact
    "Email: eng@arrcus.com";

  description
    "This module defines deviation statements for Arrcus
     implemention of the IETF IPFIX Packet Sampling.";

  revision 2024-02-12 {
    description "Limit destination port range";
  }

  revision 2023-07-10 {
    description "Initial Revision";

    reference
      "RFC 6728: https://datatracker.ietf.org/doc/html/rfc6728";
  }

  deviation "/ietf-ipfix:ipfix/ietf-ipfix:collectingProcess" {
    deviate not-supported;
    description "Not supported in this release";
  }

  deviation "/ietf-ipfix:ipfix/ietf-ipfix:observationPoint" {
    deviate add {
      max-elements 1;
    }
  }

  deviation "/ietf-ipfix:ipfix/ietf-ipfix:selectionProcess" {
    deviate add {
      max-elements 1;
    }
  }

  deviation "/ietf-ipfix:ipfix/ietf-ipfix:exportingProcess" {
    deviate add {
      max-elements 5;
    }
  }

  deviation "/ietf-ipfix:ipfix/ietf-ipfix:selectionProcess/ietf-ipfix:selector" {
    deviate add {
      max-elements 1;
    }
  }

  deviation "/ietf-ipfix:ipfix/ietf-ipfix:observationPoint/ietf-ipfix:ifIndex" {
    deviate not-supported;
    description "Not supported in this release";
  }

  deviation "/ietf-ipfix:ipfix/ietf-ipfix:observationPoint/ietf-ipfix:ifName" {
    deviate not-supported;
    description "Not supported in this release";
  }

  deviation "/ietf-ipfix:ipfix/ietf-ipfix:observationPoint/ietf-ipfix:entPhysicalIndex" {
