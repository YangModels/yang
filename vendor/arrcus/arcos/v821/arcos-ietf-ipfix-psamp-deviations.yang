  }
  deviation "/ietf-ipfix:ipfix/ietf-ipfix:exportingProcess/ietf-ipfix:options" {
    deviate not-supported;
    description "Not supported in this release";
  }

  deviation "/ietf-ipfix:ipfix/ietf-ipfix:exportingProcess/ietf-ipfix:destination/ietf-ipfix:DestinationParameters/ietf-ipfix:udpExporter/ietf-ipfix:udpExporter/ietf-ipfix:transportSession/ietf-ipfix:destinationPort" {
    deviate replace {
      type inet:port-number {
	      range "1024..65535";
      }
    }
  }

  deviation "/ietf-ipfix:ipfix/ietf-ipfix:exportingProcess/ietf-ipfix:destination/ietf-ipfix:DestinationParameters/ietf-ipfix:udpExporter/ietf-ipfix:udpExporter/ietf-ipfix:destinationPort" {
    deviate replace {
      type inet:port-number {
	      range "1024..65535";
      }
    }
  }

  deviation "/ietf-ipfix:ipfix/ietf-ipfix:observationPoint/ietf-ipfix:direction" {
    deviate replace {
      default ingress;
    }
  }

  deviation "/ietf-ipfix:ipfix/ietf-ipfix:exportingProcess/ietf-ipfix:destination" {
    deviate add {
      max-elements 1;
    }
  }

  deviation "/ietf-ipfix:ipfix/ietf-ipfix:exportingProcess/ietf-ipfix:exportMode" {
    deviate not-supported;
    description "Not supported in this release";
  }

  deviation "/ietf-ipfix:ipfix/ietf-ipfix:exportingProcess/ietf-ipfix:destination/ietf-ipfix:DestinationParameters/ietf-ipfix:udpExporter/ietf-ipfix:udpExporter/ietf-ipfix:maxPacketSize" {
    deviate not-supported;
    description "Not supported in this release";
  }

  deviation "/ietf-ipfix:ipfix/ietf-ipfix:selectionProcess/ietf-ipfix:selector/ietf-ipfix:Method/ietf-ipfix:sampCountBased/ietf-ipfix:sampCountBased/ietf-ipfix:packetInterval" {
    deviate add {
      default 1;
    }
    deviate replace {
      mandatory false;
    }
  }
