  }

  deviation "/ietf-ipfix:ipfix/ietf-ipfix:selectionProcess/ietf-ipfix:selector/ietf-ipfix:Method/ietf-ipfix:sampUniProb" {
    deviate not-supported;
    description "Not supported in this release";
  }

  deviation "/ietf-ipfix:ipfix/ietf-ipfix:selectionProcess/ietf-ipfix:selector/ietf-ipfix:Method/ietf-ipfix:filterMatch" {
    deviate not-supported;
    description "Not supported in this release";
  }

  deviation "/ietf-ipfix:ipfix/ietf-ipfix:selectionProcess/ietf-ipfix:selector/ietf-ipfix:Method/ietf-ipfix:filterHash" {
    deviate not-supported;
    description "Not supported in this release";
  }

  deviation "/ietf-ipfix:ipfix/ietf-ipfix:selectionProcess/ietf-ipfix:selector/ietf-ipfix:selectorDiscontinuityTime" {
    deviate not-supported;
    description "Not supported in this release";
  }

  deviation "/ietf-ipfix:ipfix/ietf-ipfix:selectionProcess/ietf-ipfix:selectionSequence" {
    deviate not-supported;
    description "Not supported in this release";
  }

  deviation "/ietf-ipfix:ipfix/ietf-ipfix:selectionProcess/ietf-ipfix:cache" {
    deviate not-supported;
    description "Not supported in this release";
  }

  deviation "/ietf-ipfix:ipfix/ietf-ipfix:cache" {
    deviate not-supported;
    description "Not supported in this release";
  }

  deviation "/ietf-ipfix:ipfix/ietf-ipfix:exportingProcess/ietf-ipfix:destination/ietf-ipfix:DestinationParameters/ietf-ipfix:sctpExporter" {
    deviate not-supported;
    description "Not supported in this release";
  }

  deviation "/ietf-ipfix:ipfix/ietf-ipfix:exportingProcess/ietf-ipfix:destination/ietf-ipfix:DestinationParameters/ietf-ipfix:udpExporter/ietf-ipfix:udpExporter/ietf-ipfix:indexOrName" {
    deviate not-supported;
    description "Not supported in this release";
  }

  deviation "/ietf-ipfix:ipfix/ietf-ipfix:exportingProcess/ietf-ipfix:destination/ietf-ipfix:DestinationParameters/ietf-ipfix:udpExporter/ietf-ipfix:udpExporter/ietf-ipfix:sendBufferSize" {
    deviate not-supported;
    description "Not supported in this release";
  }

  deviation "/ietf-ipfix:ipfix/ietf-ipfix:exportingProcess/ietf-ipfix:destination/ietf-ipfix:DestinationParameters/ietf-ipfix:udpExporter/ietf-ipfix:udpExporter/ietf-ipfix:rateLimit" {
    deviate not-supported;
    description "Not supported in this release";
  }

  deviation "/ietf-ipfix:ipfix/ietf-ipfix:exportingProcess/ietf-ipfix:destination/ietf-ipfix:DestinationParameters/ietf-ipfix:udpExporter/ietf-ipfix:udpExporter/ietf-ipfix:transportLayerSecurity" {
    deviate not-supported;
    description "Not supported in this release";
  }

  deviation "/ietf-ipfix:ipfix/ietf-ipfix:exportingProcess/ietf-ipfix:destination/ietf-ipfix:DestinationParameters/ietf-ipfix:udpExporter/ietf-ipfix:udpExporter/ietf-ipfix:templateRefreshTimeout" {
    deviate not-supported;
    description "Not supported in this release";
  }
  deviation "/ietf-ipfix:ipfix/ietf-ipfix:exportingProcess/ietf-ipfix:destination/ietf-ipfix:DestinationParameters/ietf-ipfix:udpExporter/ietf-ipfix:udpExporter/ietf-ipfix:optionsTemplateRefreshTimeout" {
    deviate not-supported;
    description "Not supported in this release";
  }

  deviation "/ietf-ipfix:ipfix/ietf-ipfix:exportingProcess/ietf-ipfix:destination/ietf-ipfix:DestinationParameters/ietf-ipfix:udpExporter/ietf-ipfix:udpExporter/ietf-ipfix:templateRefreshPacket" {
    deviate not-supported;
    description "Not supported in this release";
  }

  deviation "/ietf-ipfix:ipfix/ietf-ipfix:exportingProcess/ietf-ipfix:destination/ietf-ipfix:DestinationParameters/ietf-ipfix:udpExporter/ietf-ipfix:udpExporter/ietf-ipfix:optionsTemplateRefreshPacket" {
