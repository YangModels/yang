            type uint32 {
              range "1..100" {
                description
                  "Throttle blocking period";
              }
            }
            must "../throttle and ../throttle-request-period";
            description
              "Throttle blocking period";
          }
        }
        container circuit-id-and-remote-id {
          description
            "Limit or throttle PPPoE sessions by circuit ID and remote ID";
          container limit {
            description
              "Limit PPPoE sessions with matching circuit ID and remote ID";
            leaf session-limit {
              type union {
                type uint32 {
                  range "1..128000" {
                    description
                      "Circuit-ID and Remote-ID session limit";
                  }
                }
                type uint32 {
                  range "1..65535" {
                    description
                      "Circuit-ID and Remote-ID session limit";
                  }
                }
              }
              description
                "Circuit-ID and Remote-ID session limit";
            }
            leaf threshold {
              type union {
                type uint32 {
                  range "1..128000" {
                    description
                      "Warning message threshold";
                  }
                }
                type uint32 {
                  range "1..65535" {
                    description
                      "Warning message threshold";
                  }
                }
              }
              must "../session-limit";
              description
                "Specify the threshold after which a warning will be displayed";
            }
          }
          leaf throttle {
            type uint32 {
              range "1..65535" {
                description
                  "Number of requests at which to throttle";
              }
            }
            must "../throttle-blocking-period and ../throttle-request-period";
            description
              "Throttle PPPoE sessions matching circuit ID and remote ID";
          }
          leaf throttle-request-period {
            type uint32 {
              range "1..100" {
                description
                  "Throttle request period";
              }
            }
            must "../throttle and ../throttle-blocking-period";
            description
              "Throttle request period";
          }
          leaf throttle-blocking-period {
            type uint32 {
              range "1..100" {
                description
                  "Throttle blocking period";
              }
            }
            must "../throttle and ../throttle-request-period";
            description
              "Throttle blocking period";
          }
        }
      }
      container ac {
        description
          "Modify Access Concentrator config";
        leaf name {
          type xr:Cisco-ios-xr-string {
            length "1..256" {
              description
                "New name";
            }
          }
          description
            "Change the name to include in the AC tag";
        }
      }
      container service {
        description
          "Modify service config";
        container service-names {
          description
            "Change the service name";
          list service-name {
            key "name";
            description
              "Change the service name";
            leaf name {
              type xr:Cisco-ios-xr-string {
                length "1..256" {
                  description
                    "New name";
                }
              }
              description
                "Change the service name";
            }
          }
        }
        container selection {
          description
            "Disable advertising of unrequested service names";
          container disable {
            presence "Indicates a disable node is configured.";
            description
              "Disable advertising of unrequested service names";
          }
        }
      }
      container tag {
        description
          "Change tag configuration";
        container ppp-max-payload {
          description
            "Change the ppp-max-payload configuration";
          container deny {
            presence "Indicates a deny node is configured.";
            description
              "Ignore the ppp-max-payload tag";
          }
          leaf minimum {
            type uint32 {
              range "500..2000" {
                description
                  "Minimum payload";
              }
            }
            must "../maximum";
            description
              "Configure minimum payload";
          }
          leaf maximum {
            type uint32 {
              range "500..2000" {
                description
                  "Maximum payload";
              }
            }
            must "../minimum";
            description
              "Configure maximum payload";
          }
        }
      }
      container pppoe {
        description
          "Process PPPoE packets";
        container padr {
          description
            "PADR packet";
          container invalid-payload {
            description
              "Invalid-payload in PADR packet";
            container allow {
              presence "Indicates a allow node is configured.";
              description
                "Allow sessions to come up";
            }
          }
          container session-unique {
            description
              "session-unique";
            container relay-session-id {
              presence "Indicates a relay-session-id node is configured.";
              description
                "Allow sessions to come up with unique relay-session-id in padr";
            }
          }
        }
      }
      container invalid-session-id {
        description
          "Specify behaviour for when a packet with an invalid session ID is received";
        container log {
          must "not(../drop)";
          presence "Indicates a log node is configured.";
          description
            "Log packets with an invalid session ID.";
        }
        container drop {
          must "not(../log)";
          presence "Indicates a drop node is configured.";
          description
            "Silently drop packets with an invalid session ID.";
        }
      }
      leaf mtu {
        type uint32 {
          range "500..2000" {
            description
              "Default maximum payload";
          }
        }
        description
          "Change default maximum payload";
      }
      container timeout {
        description
          "Change session completion timeout.";
        leaf completion {
          type uint32 {
            range "30..600" {
              description
                "Timeout period in seconds.";
            }
          }
          description
            "Change session completion timeout.";
        }
      }
      container padt-after-shutdown {
        presence "Indicates a padt-after-shutdown node is configured.";
        description
          "PADT is sent after ppp shutdown";
      }
      container padi {
        description
          "Configure the response to PADIs coming from existing hosts";
        container host-uniq-disconnect {
          presence "Indicates a host-uniq-disconnect node is configured.";
          description
            "Disconnect the session when a padi received with existing host_uniq session";
        }
      }
    }
    container session-id {
      description
        "Disable per-parent session ID spaces";
      container space {
        description
          "Disable per-parent session ID spaces";
        container flat {
          presence "Indicates a flat node is configured.";
          description
            "Disable per-parent session ID spaces";
        }
      }
    }
    leaf in-flight-window {
      type uint32 {
        range "1..20000" {
          description
            "PPPoE in-flight window size";
        }
      }
      description
        "Set the PPPoE in-flight window size";
    }
  }
}
