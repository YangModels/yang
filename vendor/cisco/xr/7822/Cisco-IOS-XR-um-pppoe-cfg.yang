      leaf bba-group-name {
        type union {
          type string {
            pattern "global" {
              description
                "Configures global bba-group for the device.";
            }
          }
          type xr:Cisco-ios-xr-string {
            length "1..32" {
              description
                "Enter group name";
            }
          }
        }
        description
          "BBA-Group name";
      }
      container control-packets {
        description
          "Change control-packets configuration";
        leaf priority {
          type uint32 {
            range "0..7" {
              description
                "Priority bits for outgoing PPPoE and PPP control packets";
            }
          }
          description
            "Set the CoS Priority to use in PPPoE and PPP control packets.";
        }
      }
