              }
              leaf delay {
                type uint32 {
                  range "0..10000" {
                    description
                      "PADO delay (in milliseconds)";
                  }
                }
                mandatory true;
                description
                  "Delay the PADO response when there is an exact match on the received Circuit ID";
              }
            }
          }
          container circuit-id-substrings {
            description
              "Delay the PADO response when the received Circuit ID contains the given string";
            list circuit-id-substring {
              key "substring";
              description
                "Delay the PADO response when the received Circuit ID contains the given string";
              leaf substring {
                type xr:Cisco-ios-xr-string {
                  length "1..1024" {
                    description
                      "The string to be contained within the received Circuit ID";
                  }
                }
                description
                  "The string to be contained within the received Circuit ID";
              }
              leaf delay {
                type uint32 {
                  range "0..10000" {
                    description
                      "PADO delay (in milliseconds)";
                  }
                }
                description
                  "Delay the PADO response when the received Circuit ID contains the given string";
