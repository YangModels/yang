        description "PIM IFRS interface information";
        leaf interface-name {
          xr:xr-xml-map "ipv4_pim_common_oper:InterfaceName";
          type xr:Interface-name;
          description "Interface name";
        }
        uses PIM-IDB-BAG;
      }
    }
  }

  grouping PROCESS {
    description "Common node of active, standby";

    container process {
      xr:xr-xml-map "ipv4_pim_role_lvl_oper:Process";
      description "Process";

      container checkpoint-statistics {
        xr:xr-xml-map "ipv4_pim_common_oper:CheckpointStatistics";
