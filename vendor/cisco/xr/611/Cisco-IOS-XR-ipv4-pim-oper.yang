      list jp-statistic {
        xr:xr-xml-map "ipv4_pim_common_oper:JPStatistics";
        key "interface-name";
        description "PIM Join Prune Statistics";
        leaf interface-name {
          xr:xr-xml-map "ipv4_pim_common_oper:InterfaceName";
          type xr:Interface-name;
          description "Interface name";
