  grouping TUNNEL-TABLE {
    description "Common node of vrf, default-context";

    container tunnels {
      xr:xr-xml-map "ipv4_pim_common_oper:TunnelTable";
      description "Tunnels information that PIM has";

      list tunnel {
        xr:xr-xml-map "ipv4_pim_common_oper:Tunnel";
        key "tunnel-name";
        description "PIM tunnel information";
        leaf tunnel-name {
