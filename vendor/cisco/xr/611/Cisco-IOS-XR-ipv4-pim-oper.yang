      }

      container bsr-elections {
        xr:xr-xml-map "ipv4_pim_common_oper:BSRElectionTable";
        description "PIM BSR election state";

        list bsr-election {
          xr:xr-xml-map "ipv4_pim_common_oper:BSRElection";
