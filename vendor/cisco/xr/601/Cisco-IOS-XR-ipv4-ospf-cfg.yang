      }

      list prefix-and-netmask {
        xr:xr-xml-map "ipv4-ospf_cfg:SummaryPrefix";
        xr:xr-cli-map "ospf <string> vrf <string> (summary-address)"+
          "|(summary-prefix) <IP address mask> [not-advertise|tag <"+
          "number>] ";
