      container max-metric-always {
        xr:xr-xml-map "ipv4-ospf_cfg:MaxMetricAlways";
        description "Set maximum metric always configuration";
        leaf include-stub {
          xr:xr-xml-map "ipv4-ospf_cfg:IncludeStub";
          type boolean;
          description
            "Set maximum metric for stub links in
