      leaf dispatch-incoming {
        xr:xr-xml-map "ipv4-ospf_cfg:DispatchIncoming";
        xr:xr-cli-map "ospf <string> vrf <string> queue dispatch in"+
          "coming <number> ";
        type uint32 {
          range "30..3000";
        }
        default "300";
        description
          "Maximum number of continuous incoming
          packet-related events processed";
