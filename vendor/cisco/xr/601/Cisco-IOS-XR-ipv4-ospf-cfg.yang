      }
      leaf key {
        xr:xr-xml-map "ipv4-ospf_cfg:Key";
        xr:xr-cli-map "ospf <string> vrf <string> authentication-ke"+
          "y ([0] <text>)|(clear)|((7)|(encrypted) <text>)|(<text>)"+
          " ";
