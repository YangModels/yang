          description
            "Let BGP decide when to originate router-LSA
            with normal metric";
        }
        leaf startup-max {
          xr:xr-xml-map "ipv4-ospf_cfg:StartupMax";
          type uint32 {
            range "5..86400";
          }
          units "second";
          description
