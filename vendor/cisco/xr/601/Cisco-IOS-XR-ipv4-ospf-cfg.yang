        list message-digest-key {
          xr:xr-xml-map "ipv4-ospf_cfg:MessageDigestKey";
          xr:xr-cli-map "ospf <string> vrf <string> message-digest-"+
            "key <number> md5 ([0] <text>)|(clear)|((7)|(encrypted)"+
            " <text>)|(<text>) ";
