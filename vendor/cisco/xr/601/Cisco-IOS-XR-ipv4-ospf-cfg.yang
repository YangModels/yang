      leaf dispatch-spf-lsa-limit {
        xr:xr-xml-map "ipv4-ospf_cfg:DispatchSPF_LSALimit";
        xr:xr-cli-map "ospf <string> vrf <string> queue dispatch sp"+
          "f-lsa-limit <number> ";
