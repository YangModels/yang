  revision 2021-12-08 {
    description
      "Added name and tx-interval nodes support under delay-profile and liveness-profile";
    semver:module-version "1.1.0";
  }
  revision 2021-06-29 {
    description
      "Initial release";
    semver:module-version "1.0.0";
