    /////////////////////////////////////////////////////////
    // native / redundancy *
    /////////////////////////////////////////////////////////
    container redundancy { 
      description
        "Enter redundancy mode";
      presence true;
      //application redundancy
      container application {
        description
          "Enter application redundancy group mode";
        container redundancy {
          description 
            "Enter application redundancy group mode";
          presence true;
          list group {
            description
              "Enter rg group mode";
            key id;
            leaf id {
              description
                "rg group id";

              type uint8 {
                  range "1..2";
              }
            }

            container name {
              description
                "RG group name string";

              leaf group-name {
                description
                  "name string (25 chars. max)";
                type string {
                  length "1..25";
                }
              }
            }
            container control {
              description
                "RG group control interface";
