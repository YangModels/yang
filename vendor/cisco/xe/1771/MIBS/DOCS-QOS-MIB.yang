    container docsQosServiceClassPolicyTable {
      description
       "This table describes the set of Docsis-QOS 
        Service Class Policies.  
        
        This table is an adjunct to the
        docsDevFilterPolicy table.  Entries in 
        docsDevFilterPolicy table can  point to 
        specific rows in this table.
        
        This table permits mapping a packet to a service
        class name of an active service flow so long as 
        a classifier does not exist at a higher
        priority.";
      reference
       "SP-RFIv1.1-I07-010829, Appendix E.2.1";
      smiv2:oid "1.3.6.1.2.1.10.127.7.1.9";

      list docsQosServiceClassPolicyEntry {
        key "docsQosServiceClassPolicyIndex";
        description
         "A service class name policy entry.";
        smiv2:oid "1.3.6.1.2.1.10.127.7.1.9.1";

        leaf docsQosServiceClassPolicyIndex {
          type int32 {
            range "1..2147483647";
          }
          description
           "Index value to uniquely identify an entry in
            this table.";
          smiv2:max-access "not-accessible";
          smiv2:oid "1.3.6.1.2.1.10.127.7.1.9.1.1";
        }

        leaf docsQosServiceClassPolicyName {
          type snmpv2-tc:DisplayString;
          description
           "Service Class Name to identify the name of the 
            service class flow to which the packet should be
            directed.";
