           "SP-RFIv1.1-I07-010829, Appendix E.2.1";
          smiv2:max-access "read-write";
          smiv2:oid "1.3.6.1.2.1.10.127.7.1.9.1.2";
        }

        leaf docsQosServiceClassPolicyRulePriority {
