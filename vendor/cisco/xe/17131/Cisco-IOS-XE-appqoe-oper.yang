  typedef appqoe-tcpopt-run-status {
    type enumeration {
      enum appqoe-tcpopt-running {
        value 0;
        description
          "TCP is running";
      }
      enum appqoe-tcpopt-not-running {
        value 1;
        description
          "TCP is not running";
      }
    }
    description
