          reference
           "DOCSIS Baseline Privacy Plus Interface Specification,
            Section 4.2.1.8.";
          smiv2:max-access "read-only";
          smiv2:oid "1.3.6.1.2.1.126.1.2.3.1.13";
        }

        leaf docsIetfBpi2CmtsKeyRejectErrorCode {
          type enumeration {
            enum "none" {
              value "1";
