            This object value persist after re-initialization of the
            managed system.";
          reference
           "DOCSIS Baseline Privacy Plus Interface Specification,
            Appendix A.2.";
          smiv2:defval "604800";
          smiv2:max-access "read-write";
          smiv2:oid "1.3.6.1.2.1.126.1.2.1.1.1";
        }

        leaf docsIetfBpi2CmtsDefaultTEKLifetime {
          type int32 {
            range "1..604800";
