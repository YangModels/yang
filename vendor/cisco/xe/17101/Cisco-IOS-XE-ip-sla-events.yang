      }
      enum percentile-avg-latency-sd {
        value 34;
        description
          "Monitoring of percentile latency average in the
           direction from source to destination";
      }
      enum percentile-avg-latency-ds {
        value 35;
        description
          "Monitoring of percentile latency average in the
