      }
      enum udp-echo-oper-data {
        value 12;
        description
          "IPSLA UDP-echo operation data";
      }
      enum udp-jitter-oper-data {
        value 13;
        description
          "IPSLA UDP-jitter operation data";
      }
      enum y1731-delay-oper-data {
