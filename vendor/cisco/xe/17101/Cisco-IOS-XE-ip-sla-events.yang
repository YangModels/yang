      }
      enum max-of-latency-sd {
        value 24;
        description
          "Monitoring of maximum latency in the direction
           from source to destination";
      }
      enum avg-latency-ds {
        value 25;
        description
          "Monitoring of average latency in the direction
           from destination to source";
      }
      enum avg-latency-sd {
        value 26;
        description
          "Monitoring of average latency in the direction
