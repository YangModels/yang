    uses l0-types:flexi-grid-link-bandwidth;
  }
}

/* Augment bandwidth path constraints of connectivity-matrices
   information-source */
augment "/nw:networks/nw:network/nw:node/tet:te/"
      + "tet:information-source-entry/tet:connectivity-matrices/"
